page{
  width: 100%;
  height: 100%;
}
.container{
  background-color: #55AAAD;
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.lists{
  display: flex;
}
.lists>view .item{
  flex: 1;
}
.lists .item{
  position: relative;
}
.lists .item image{
  width: 150rpx;
  height: 150rpx;
}
.item .timetshow{
  position: absolute;
  top: 125rpx;
  left: 10rpx;
  width: 130rpx;
  text-align: center;
  font-size: 20rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.3) 
}
.item .timethidden{
  display: none;
}
.lists .floor{
  color: #fff;
  align-content: center;
  margin: auto 30rpx;
  padding: 10rpx 0;
}
.lists .floor.checked{
  border-top: 2rpx solid #FF9676;
  border-bottom: 2rpx solid #FF9676;
}